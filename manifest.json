{
    "manifest_version": 2,
    "name": "lacewing",
    "version": "1.0",
    "description": "Shows element outlines, and tag info (on hover).",
    "homepage_url": "https://github.com/commitsovercoffee/lacewing",
    "icons": {
        "48": "icons/icon-48.svg"
    },
    "permissions": ["activeTab", "tabs"],
    "browser_action": {
        "default_icon": "icons/icon-48.svg",
        "default_title": "Show Outlines"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
